<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zdy.springmybatis.StudentMapper">
    <select id="getStudentByID" resultType="com.zdy.springmybatis.Student" >
        select * from tb_Student t where t.stuID = #{id}
    </select>
</mapper>
